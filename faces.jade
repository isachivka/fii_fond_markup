extend layout/base

block content
  include partials/header
  .b_promo.m_little(style='background-image: url(images/promo_1.png)')
    .b_layout
      .typo_sh.vam президиум и учредители фонда
      .vam_helper
  .b_main_member
    .b_layout
      img(src='images/main_member.png')
      .name АРтемов алексей валерьевич
      .role Исполнительный директор
  .b_layout
    .b_members
      .title.typo_sh Члены президиума
      .b_member
        .pure-g
          aside.pure-u-6-24
            img(src='images/member_1.png')
